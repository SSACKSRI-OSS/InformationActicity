<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.professionalandroid.apps.first">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.First">
        <activity android:name=".StiropomActivity"></activity>
        <activity android:name=".CanActivity" />
        <activity android:name=".VinylActivity" />
        <activity android:name=".PaperActivity" />
        <activity android:name=".GlassActivity" />
        <activity android:name=".PlasticActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>